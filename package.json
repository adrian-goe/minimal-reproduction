    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "1.19.1",
    "protractor": "~5.4.0",
    "tailwindcss": "1.5.2",
    "ts-node": "7.0.1",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "url-loader": "^3.0.0",
    "webpack-cli": "^3.3.10"
